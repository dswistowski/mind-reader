{
  "manifest_version": 2,
  "name": "New tab page with handy history search",
  "description": "Search history as default action to ",
  "author": "damian@swistowski.org",
  "version": "0.0.1",
  "permissions": [
    "https://www.google.com/search/*",
    "history",
    "storage",
    "alarms"

  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}