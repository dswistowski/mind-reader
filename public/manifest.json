{
  "manifest_version": 2,
  "name": "\uD83E\uDDE0 mind reader",
  "description": "New tab page with handy history search",
  "author": "damian@swistowski.org",
  "version": "0.0.1",
  "permissions": [
    "history",
    "storage",
    "alarms"
  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}